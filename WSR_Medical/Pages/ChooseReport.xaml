<Page x:Class="WSR_Medical.Pages.ChooseReport"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WSR_Medical.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ChooseReport">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <Button Height="70" Width="300" Content="Контроль качества" Click="ToQualityControl"/>
        <Button Height="70" Width="300" Content="Отчёт по услугам" Grid.Row="1" Click="ToServiceReport"/>
    </Grid>
</Page>
